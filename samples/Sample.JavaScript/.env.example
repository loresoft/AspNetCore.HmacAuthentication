# HMAC Authentication Configuration
# Copy this file to .env and update with your actual values

# The client identifier used for HMAC authentication
HMAC_CLIENT_ID=your-client-id

# The secret key for HMAC signing (base64 encoded)
HMAC_SECRET=your-secret-key

# The base URL of the API
API_BASE_URL=https://localhost:7134

# Development Settings
# Set to 0 to disable TLS certificate validation for development
NODE_TLS_REJECT_UNAUTHORIZED=0
